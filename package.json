{
  "name": "vehicle-management-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "PostgreSQL backend for vehicle management system",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "dev:frontend": "vite --port 8080",
    "dev:frontend:clean": "call kill-frontend.bat && vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "dev:full:clean": "call kill-frontend.bat && concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "dev:clear-cache": "node scripts/dev-clear-cache.js",
    "kill:frontend": "call kill-frontend.bat",
    "migrate": "node src/database/migrate.js"
  },
